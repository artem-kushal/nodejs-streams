{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": "off",
        "max-len": [
            "error",
            120,
            {
                "ignoreStrings": true,
                "ignoreRegExpLiterals": true
            }
        ],
        "no-mixed-operators": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "no-await-in-loop": "off",
        "no-restricted-syntax": "off",
        "camelcase": "off"
    }
}
